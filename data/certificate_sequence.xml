<?xml version="1.6" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
        <record id="gp_certificate_sequence" model="ir.sequence">
            <field name="name">Certificate ID</field>
            <field name="code">gp.exam.schedule</field>
            <field name="prefix">GP/</field>
            <field name="padding">5</field>
            <field name="number_next_actual">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">False</field>
        </record>

        <record id="ccmc_certificate_sequence" model="ir.sequence">
            <field name="name">Certificate ID</field>
            <field name="code">ccmc.exam.schedule</field>
            <field name="prefix">CCMC/</field>
            <field name="padding">5</field>
            <field name="number_next_actual">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">False</field>
        </record>
    </data>
</odoo>
