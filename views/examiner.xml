<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="bes_examiner_action" model="ir.actions.act_window">
            <field name="name">Examiner</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">bes.examiner</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">Examiner</p>
            </field>
        </record>

        <record id="bes_examiner_form" model="ir.ui.view">
            <field name="name">bes.examiner.form</field>
            <field name="model">bes.examiner</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                            <field name="state" widget="statusbar"/>
                    </header>
                    <sheet>
                        <group>
                            <field nolabel="1" name="examiner_image" widget="image" options="{'preview_image': 'image'}" width="150" height="150"/>
                        </group>
                        <group style="width:100%;">
                            <h1>
                                <field name="name" placeholder="Examiner Name"/>
                            </h1>
                        </group>
                        <group style="width:100%;">
                            <field name="subject_id" string="Subject" />
                            <field name="exam_center"  />
                        </group>
                        <group>
                            <group>
                                <span class="o_form_label o_td_label">
                                    <b>Address</b>
                                </span>
                                <div class="o_address_format">
                                    <field name="street" placeholder="Street..." class="o_address_street" />
                                    <field name="street2" placeholder="Street 2..." class="o_address_street"/>
                                    <field name="city" placeholder="City" class="o_address_city" />
                                    <field name="state_id" class="o_address_state" placeholder="State" options="{'no_open': True, 'no_quick_create': True}"/>
                                    <field name="zip" placeholder="ZIP" class="o_address_zip" />
                                </div>
                            </group>
                            <group >
                                <field name="phone" widget="phone"/>
                                <field name="mobile" widget="phone"/>
                                <field name="email"/>
                                <field name="pan_no"/>
                                <field name="dob"/>
                            </group>
                        </group>
                        <group>
                            <field name="present_designation"/>
                            <field name="name_address_present_employer"/>
                        </group>
                        <group>
                            <field name="designation"/>
                            <field name="invigilator"/>
                        </group>
                        <group style="width:50%;">
                            <field name="competency_no"/>
                        </group>
                        <group style="width:50%;">
                            <field name="date_of_issue" />
                        </group>
                        <group style="width:50%;">
                            <field name="member_of_imei_cmmi"/>
                        </group>
                        <group style="width:50%;">
                            <field name="membership_no" attrs="{'invisible': [('member_of_imei_cmmi', '=','no')] , 'required':[('member_of_imei_cmmi','=','yes')]}"/>
                        </group>
                        <group>
                            <field name="institute_association"/>
                        </group>
                        <group style="width:50%;">
                            <field name="associated_training_institute" attrs="{'invisible': [('institute_association', '=',False)] }"/>
                        </group>

                        <group>
                            <field name="present_employer_clearance" />
                        </group>

                        <group style="width:50%;">
                            <field name="exam_coordinator" />
                        </group>
                        <notebook>
                            <!-- <page string="Assignment of Examinations">
                                <field name="assignments">
                                    <tree>
                                        <field name="examiner_id" />
                                        <field name="assignment_date" />
                                        <field name="exam_start_time" />
                                        <field name="exam_end_time" />
                                        <field name="assigned_to" />
                                        <field name="subject_id" />
                                        <field name="gp_candidates" attrs="{'invisible': [('assigned_to', '!=', 'gp_candidate')]}" />
                                        <field name="ccmc_candidates" attrs="{'invisible': [('assigned_to', '!=', 'ccmc_candidate')]}" />
                                    </tree>
                                </field>
                            </page> -->
                            <page string="Payment Details">
                                <group style="width:50%;">
                                    <field name="acc_no" string="Account No."/>
                                    <field name="ifsc_code" string="IFSC Code"/>
                                    <field name="bank_name" string="Bank Name"/>                        
                                </group>
                            </page>
                        </notebook>

                        <!-- <field name="assignments">
                            <tree >
                                <field name="examiner_id" />
                                <field name="assignment_date" />
                                <field name="exam_start_time" />
                                <field name="exam_end_time" />
                                <field name="assigned_to" />
                                <field name="subject_id" />
                                <field name="gp_candidates" />
                                <field name="ccmc_candidates" />
                            </tree>
                        </field> -->




                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name= "message_ids" widget="mail_thread"/>
                        <field name= "activity_ids" widget="mail_activity"/>
                    </div>
                </form>
            </field>
        </record>

        <record id="bes_examiner_search" model="ir.ui.view">
            <field name="name">bes.examiner.search</field>
            <field name="model">bes.examiner</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                    
                    <field name="mobile"/>
                    <field name="email"/>
                    <field name="pan_no"/>
                    <field name="subject_id"/>
                    <field name="exam_center"/>
                </search>
            </field>
        </record>

        <record id="bes_examiner_tree" model="ir.ui.view">
            <field name="name">bes.examiner.tree</field>
            <field name="model">bes.examiner</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="state" widget="badge" decoration-success= "state == 'active'"  decoration-danger= "state == 'inactive'" optional="show"/>
                    <field name="exam_center"  optional="show"/>
                    <field name="mobile"  optional="show"/>
                    <field name="phone"  optional="show"/>
                    <field name="pan_no"  optional="show"/>
                    <field name="dob"  optional="show"/>
                    <field name="present_designation"  optional="show"/>
                    <field name="designation"  optional="show"/>
                    <field name="competency_no"  optional="show"/>
                    <field name="member_of_imei_cmmi"  optional="show"/>
                    <field name="membership_no"  optional="show"/>
                    <field name="date_of_issue"  optional="show"/>
                    <!-- <field name="exam_assignments"/> -->
    
                </tree>
            </field>
        </record>

        <record id="bes_examiner_oral_practical_duty_action" model="ir.actions.act_window">
            <field name="name">Examiner Duty</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">exam.type.oral.practical</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">Examiner Duty</p>
            </field>
        </record>


        <record id="examiner_assignment_wizard_form" model="ir.ui.view">
            <field name="name">examiner.assignment.wizard.form</field>
            <field name="model">examiner.assignment.wizard</field>
            <field name="arch" type="xml">
                <form string="Example Wizard">
                    <group>
                        <field name="exam_duty" invisible="1"/>
                        <field name="exam_region" invisible="1"/>
                        <field name="institute_id" domain="[('exam_center','=', exam_region)]"/>
                        <field name="course"/>
                        
                    </group>
                    <group style="width: 50%">
                        <field name="gsk_prac_oral_candidates"/>
                        <field name="mek_prac_oral_candidates"/>
                    </group>
                     <group style="width: 50%">
                 
                        <field name="gsk_online_candidates"/>
                        <field name="mek_online_candidates"/>
                    </group>

                    <group>
                        <field name="no_of_days"/>
                    </group>

                    <group colspan="8" col="4">
                        <field name="examiner_required_gsk"/>
                        <field name="examiner_required_mek"/>
                    </group>

                    

                    
                    
                    <field name="examiner_lines_ids" >
                        <tree editable="bottom">
                            <field name="exam_date" />
                            <field name="subject" domain="[('course_id','=', parent.course)]" options="{'no_create':True}"/>
                            <field name="exam_type"/>
                            <field name="examiner" domain="[('exam_center','=', parent.exam_region)]" options="{'no_create':True}"/>
                            <field name="gp_marksheet_ids"  options="{'no_create':True}"/>
                            <field name="no_candidates"/>
                        </tree>
                    </field>
                   
                    <button string="Update" type="object" class="oe_highlight" name="update_marksheet"/>
                    <group>
                        <span>Click on The Update Button to Update the Candidate After Adding Exam Date,Subject and Exam Type in List Above</span>  
                    </group>
                    
                    <footer>
                        <button string="Confirm" type="object" name="confirm" class="btn-primary"/>
                        <group>
                            <span>Click on The Confirm Button after you are satified with the assignment of the examiners</span>  
                        </group>
                    </footer>
                </form>
            </field>
        </record>


        <record id="ccmc_examiner_assignment_wizard_form" model="ir.ui.view">
            <field name="name">ccmc.examiner.assignment.wizard.form</field>
            <field name="model">ccmc.examiner.assignment.wizard</field>
            <field name="arch" type="xml">
                <form string="Example Wizard">
                    <group>
                        <field name="exam_duty" invisible="1"/>
                        <field name="exam_region" invisible="1"/>
                        <field name="institute_id" domain="[('exam_center','=', exam_region)]"/>
                        <field name="course"/>  
                        <!-- <field name="ccmc_gsk_oral_candidates"/> -->
                      
                    </group>
                    <group style="width: 50%">
                        <field name="ccmc_prac_oral_candidates"/>
                        <field name="ccmc_gsk_oral_candidates"/>
                    </group>
                    <group style="width: 50%">
                        <field name="ccmc_online_candidates"/>
                    </group>

                    <group>
                        <field name="no_of_days"/>
                    </group>

                    <group colspan="8" col="4">
                        <field name="examiner_required_ccmc_prac_oral"/>
                        <field name="examiner_required_ccmc_gsk_oral"/>
                    </group>
                    
                    
                    
                    <field name="examiner_lines_ids" >
                        <tree editable="bottom">
                            <field name="exam_date" />
                            <field name="subject" domain="[('course_id','=', parent.course)]" options="{'no_create':True}"/>
                            <field name="exam_type"/>
                            <field name="examiner" domain="[('exam_center','=', parent.exam_region)]" options="{'no_create':True}"/>
                            <field name="ccmc_marksheet_ids"  options="{'no_create':True}"/>
                            <field name="no_candidates"/>
                        </tree>
                    </field>
                   
                    <button string="Update" type="object" class="oe_highlight" name="update_marksheet"/>
                    <group>
                        <span>Click on The Update Button to Update the Candidate After Adding Exam Date,Subject and Exam Type in List Above</span>  
                    </group>
                    
                    <footer>
                        <button string="Confirm" type="object" class="oe_highlight" name="confirm"/>
                        <group>
                            <span>Click on The Confirm Button after you are satified with the assignment of the examiners</span>  
                        </group>
                    </footer>
                </form>
            </field>
        </record>



        <record id="bes_examiner_oral_practical_duty_action_form" model="ir.ui.view">
            <field name="name">exam.type.oral.practical.form</field>
            <field name="model">exam.type.oral.practical</field>
            <field name="arch" type="xml">
                <form>
                    <!-- <header>
                            <field name="state" widget="statusbar"/>
