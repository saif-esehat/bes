                        </group>
                        <group style="width:50%;">
                            <field name="exam_type"  required="1" />
                        </group> -->
                        
                        <button name="open_assignment_wizard" string="Add Examiner Assignment" type="object" class="btn-primary" />

                       
                        <field name="examiners" >
                            <tree editable="bottom"  >
                                <field name="prac_oral_id" invisible="1"  />
                                <field name="dgs_batch" required="1" invisible="1"   />
                                <field name="subject" invisible="0" />
                                <field name="course" required="1" invisible="1"  />
                                <field name="exam_type" invisible="0"  />
                                <field name="exam_region" invisible="1"/>
                                <field name="examiner" required="1" domain="[('exam_center','=', exam_region)]"  />
                                <field name="institute_id" required="1" domain="[('exam_center','=', exam_region)]"/>
                                <field name="team_lead" invisible="1"/>
