                                <field name="expense_sheet" />
                                <field name="status" invisible="1" />
                                <field name="marksheets"  />
                                <button string="Open" type="object" class="oe_highlight" name="open_marksheet_list"/>
                                <field name="marksheet_image" widget="binary" filename="marksheet_image_name" />
                                <field name="marksheet_uploaded"/>
                            </tree>
                        </field>



                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name= "message_ids" widget="mail_thread"/>
