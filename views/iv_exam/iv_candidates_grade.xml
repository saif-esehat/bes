<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_candidates_grade_form" model="ir.ui.view">
        <field name="name">candidates.grade.form</field>
        <field name="model">candidates.grade</field>
        <field name="arch" type="xml">
            <form string="Candidates Grade">
                <sheet>
                    <group>
                        <field name="grade"/>
                        
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_candidates_grade_tree" model="ir.ui.view">
        <field name="name">candidates.grade.tree</field>
        <field name="model">candidates.grade</field>
        <field name="arch" type="xml">
            <tree string="Candidates Grade">
                <field name="grade"/>
               
            </tree>
        </field>
    </record>
    <record id="action_candidates_grade" model="ir.actions.act_window">
        <field name="name">Candidates Grade</field>
        <field name="res_model">candidates.grade</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="menu_candidates_grade" name="Candidates Grade"
              action="action_candidates_grade" parent="menu_iv_candidats_application"/>


    </data>
</odoo>